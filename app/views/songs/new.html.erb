<h2> Add Your Song Here </h2>

<%= form_tag({:controller => "songs", :action => "create"}, method: :post) do %>
    <%= label_tag "name" %>
    <%= text_field_tag "name"%>
    <%= label_tag "audio_file" %>
    <%= text_field_tag "audio_file" %>
    <%= hidden_field_tag :artist_id, session[:artist_id] %>
    <%= submit_tag "Create Song" %>

<%end%>

